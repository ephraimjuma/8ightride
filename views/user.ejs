function formatPhoneNumber(phone) {
    if (phone.startsWith('0')) {
        phone = '254' + phone.substring(1);
    } else if (phone.startsWith('7')) {
        phone = '254' + phone;
    } else if (phone.startsWith('+254')) {
        phone = phone.substring(1);
    }

    return phone;
}

    async function initiateMpesaPayment() {
            const phone = document.getElementById('inputPhone').value;
            const totalCost = document.getElementById('amountToPay').textContent.replace('Amount to Pay: ', '').replace(' KSH', '');

            console.log('Phone:', phone, 'Total Cost:', totalCost);

            const response = await fetch('/api/payment/process', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ amount: totalCost, phone })
            });

            const data = await response.json();
            console.log(data);

            if (data.ResponseCode === '0') {
      alert('Payment initiated successfully. Please check your phone to complete the payment.');
         } else {
           alert(`Payment initiation failed: ${data.errorMessage || 'Unknown error'}`);
           }
         }

