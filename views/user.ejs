<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>User Home</title>
    <link rel="icon" href="img/favicon.png">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <!-- animate CSS -->
    <link rel="stylesheet" href="css/animate.css">
    <!-- owl carousel CSS -->
    <link rel="stylesheet" href="css/owl.carousel.min.css">
    <!-- themify CSS -->
    <link rel="stylesheet" href="css/themify-icons.css">
    <!-- flaticon CSS -->
    <link rel="stylesheet" href="css/flaticon.css">
    <!-- magnific popup CSS -->
    <link rel="stylesheet" href="css/magnific-popup.css">
    <!-- nice select CSS -->
    <link rel="stylesheet" href="css/nice-select.css">
    <!-- swiper CSS -->
    <link rel="stylesheet" href="css/slick.css">
    <!-- style CSS -->
    <link rel="stylesheet" href="css/style.css">
    <style>
        .navbar-nav .nav-link {
            font-size: 18px;
            font-weight: bold;
        }

        .welcome-user {
            font-size: 18px;
            font-weight: bold;
            color: #ffffff;
        }

        .profile-icon {
            font-size: 18px;
            font-weight: bold;
            color: #ffffff;
            cursor: pointer;
            margin-left: 15px;
        }

        .profile-icon:hover,
        .nav-link:hover {
            color: #ff7f50;
        }

        .dropdown-menu .dropdown-item:hover {
            background-color: #f1f1f1;
            color: #343a40;
        }

        .profile_img img {
            border-radius: 50%;
        }

        .btn-upload {
            background-color: #4892dc;
            color: #fff;
            border: none;
            padding: 10px;
            border-radius: 5px;
            cursor: pointer;
        }

        .btn-upload:hover {
            background-color: #ff7f50;
        }
    </style>
</head>

<body>
    <!--::header part start::-->
    <header class="main_menu home_menu" style="padding: 10px 0; background-color: #4892dc;">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <a class="navbar-brand" href="#"> <img src="img/logo.png" alt="logo" style="height: 40px;"> </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse"
                            data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>

                        <div class="collapse navbar-collapse justify-content-between" id="navbarSupportedContent">
                            <div class="navbar-nav">
                                <span class="welcome-user">Welcome, <%= username %></span>
                            </div>
                            <ul class="navbar-nav">
                                <li class="nav-item active">
                                    <a class="nav-link" href="#">Home</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/complain">Complain</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="/logout">Log Out</a>
                                </li>
                                <li class="nav-item dropdown">
                                    <span class="profile-icon dropdown-toggle" id="profileDropdown"
                                        data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        <i class="ti-user"></i>
                                    </span>
                                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="profileDropdown">
                                        <a class="dropdown-item" href="#">Edit Profile</a>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- Header part end-->

    <!-- profile part start-->
    <section class="profile_part section_padding">
        <div class="container">
            <div class="row">
                <div class="col-lg-4">
                    <div class="profile_img">
                        <img src="img/profile_pic.jpg" alt="Profile Picture" style="width: 100%; border-radius: 10px;">
                        <input type="file" id="uploadProfilePic" style="display: none;">
                        <button class="btn-upload" onclick="document.getElementById('uploadProfilePic').click();">Upload Profile Picture</button>
                    </div>
                </div>
                <div class="col-lg-8">
                    <div class="profile_info">
                        <h2>User Profile</h2>
                        <p><strong>Name:</strong> <%= username %></p>
                        <p><strong>Email:</strong> johndoe@example.com</p>
                        <p><strong>Phone:</strong> +123 456 7890</p>
                        <a href="#" class="btn_2">Edit Profile</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- profile part end-->

    <!-- booking part start-->
    <section class="regervation_part section_padding">
        <div class="container">
            <div class="row align-items-center regervation_content">
                <div class="col-lg-6">
                    <div class="regervation_part_iner">
                        <form>
                            <h2>Book a Ride</h2>
                            <div class="form-row">
                                <div class="form-group col-md-6">
                                    <input type="text" class="form-control" id="inputName" placeholder="Name">
                                </div>
                                <div class="form-group col-md-6">
                                    <input type="email" class="form-control" id="inputEmail" placeholder="Email address">
                                </div>
                                <div class="form-group col-md-6">
                                    <select class="form-control" id="SelectService">
                                        <option value="1" selected>Select service</option>
                                        <option value="2">Service 1</option>
                                        <option value="3">Service 2</option>
                                        <option value="4">Service 3</option>
                                        <option value="5">Service 4</option>
                                    </select>
                                </div>
                                <div class="form-group time_icon col-md-6">
                                    <select class="form-control" id="SelectTime">
                                        <option value="" selected>Time</option>
                                        <option value="1">8 AM TO 10 AM</option>
                                        <option value="2">10 AM TO 12 PM</option>
                                        <option value="3">12 PM TO 2 PM</option>
                                        <option value="4">2 PM TO 4 PM</option>
                                        <option value="5">4 PM TO 6 PM</option>
                                        <option value="6">6 PM TO 8 PM</option>
                                        <option value="7">8 PM TO 10 PM</option>
                                        <option value="8">10 PM TO 12 AM</option>
                                    </select>
                                </div>
                                <div class="form-group col-md-12">
                                    <textarea class="form-control" id="inputNote" rows="4" placeholder="Your Note"></textarea>
                                </div>
                            </div>
                            <div class="regerv_btn">
                                <a href="#" class="btn_2">Book a Ride</a>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="reservation_img">
                        <img src="img/reservation.jpg" alt="" class="reservation_img_iner" style="width: 100%; height: auto;">
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- booking part end-->
                  
    <!-- feature part start-->
    <section class="feature_part">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-xl-8">
                    <div class="section_tittle text-center">
                        <h2>Available Services</h2>
                    </div>
                </div>
            </div>
            <div class="row justify-content-between align-items-center">
                <div class="col-lg-3 col-sm-12">
                    <div class="single_feature">
                        <div class="single_feature_part">
                            <span class="single_feature_icon"><img src="img/icon/feature_1.svg" alt=""></span>
                            <h4>Flexible Hours</h4>
                            <p>We offer flexible hours to suit your schedule.</p>
                        </div>
                    </div>
                    <div class="single_feature">
                        <div class="single_feature_part">
                            <span class="single_feature_icon"><img src="img/icon/feature_2.svg" alt=""></span>
                            <h4>Safety</h4>
                            <p>Your safety is our priority.</p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-sm-12">
                    <div class="single_feature_img">
                        <img src="img/service.png" alt="service image">
                    </div>
                </div>
                <div class="col-lg-3 col-sm-12">
                    <div class="single_feature">
                        <div class="single_feature_part">
                            <span class="single_feature_icon"><img src="img/icon/feature_1.svg" alt=""></span>
                            <h4>Familiarity</h4>
                            <p>Our drivers are familiar with all routes.</p>
                        </div>
                    </div>
                    <div class="single_feature">
                        <div class="single_feature_part">
                            <span class="single_feature_icon"><img src="img/icon/feature_2.svg" alt=""></span>
                            <h4>Cost-Effective</h4>
                            <p>We offer the best prices in town.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- feature part end-->

