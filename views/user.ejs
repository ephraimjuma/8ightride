        function confirmBooking() {
    // Gather form data
    event.preventDefault();
    var name = document.getElementById('inputName').value;
    var email = document.getElementById('inputEmail').value;
    var phone = document.getElementById('inputPhone').value;
    var service = document.getElementById('SelectService').value;
    var pickupLocation = document.getElementById('SelectPickupLocation').value;
    var dropoffLocation = document.getElementById('SelectDropoffLocation').value;
    var pickupTime = document.getElementById('SelectPickupTime').value;
    var schoolArrivalTime = document.getElementById('SelectSchoolArrivalTime').value;
    var dropoffTime = document.getElementById('SelectDropoffTime').value;
    var note = document.getElementById('inputNote').value;

    var days = [];
    var checkboxes = document.querySelectorAll('#DaysOfWeek input[type="checkbox"]:checked');
    checkboxes.forEach((checkbox) => {
        days.push(checkbox.value);
    });

    // Ensure that the mappings are used correctly
    var pickupTimeText = pickupTimeMapping[pickupTime] || 'Not specified';
    var schoolArrivalTimeText = schoolArrivalTime || 'Not specified';
    var dropoffTimeText = dropoffTime || 'Not specified';

    // Set confirmation details
    var confirmationDetails = `
        <strong>Name:</strong> ${name}<br>
        <strong>Email:</strong> ${email}<br>
        <strong>Phone:</strong> ${phone}<br>
        <strong>Service:</strong> ${serviceMapping[service]}<br>
        <strong>Pickup Location:</strong> ${locationMapping[pickupLocation]}<br>
        <strong>Dropoff Location:</strong> ${universityMapping[dropoffLocation]}<br>
        <strong>Pickup Time:</strong> ${pickupTimeText}<br>
        <strong>University Arrival Time:</strong> ${schoolArrivalTimeText}<br>
        <strong>Pick-up from University:</strong> ${dropoffTimeText}<br>
        <strong>Days:</strong> ${days.join(', ')}<br>
        <strong>Note:</strong> ${note}
    `;
    document.getElementById('confirmationDetails').innerHTML = confirmationDetails;

    // Show confirmation modal
    $('#confirmationModal').modal('show');
}

function calculateTotalCost() {
    var checkboxes = document.querySelectorAll('#DaysOfWeek input[type="checkbox"]:checked');
    var numberOfDays = checkboxes.length;
    var totalCost = 0;
    checkboxes.forEach((checkbox) => {
        totalCost += dayCost[checkbox.value];
    });

    document.getElementById('totalCost').innerHTML = `<strong>Total Cost:</strong> ${totalCost} KSH`;
    document.getElementById('amountToPay').innerHTML = `<b>Amount to Pay: ${totalCost} KSH</b>`;

    // Show amount modal
    $('#confirmationModal').modal('hide');
    $('#amountModal').modal('show');
}

